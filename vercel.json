{
    "version": 2,
    "routes": [{
            "src": "/txt",
            "dest": "/api/send_txt.js"
        },
        {
            "src": "/img",
            "dest": "/api/send_img.js"
        }
    ],
    "builds": [{
            "src": "api/send_txt.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/send_img.js",
            "use": "@vercel/node"
        }
    ]
}